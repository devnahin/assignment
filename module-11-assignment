<script>
    function multiplicationTable(num) {
  for (let i = 1; i <= 10; i++) {
    let product = num * i;
    console.log(`${num} x ${i} = ${product}`);
  }
}

// Test the function with different input numbers to generate multiplication tables
multiplicationTable(5);
console.log("------------------");
multiplicationTable(7);
</script>
